    "n8n:deploy": "npm run build:ts && node dist/deploy/deploy-n8n-workflows.js",
    "db:setup": "npm run build:ts && node dist/database/setup-tables.js",
    "deploy": "npm run build && npm run deploy:prod",
    "deploy:prod": "npm run build:ts && node dist/deploy/deploy-production.js"
  },
